#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 10;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00928F90 .scope module, "tb_memory" "tb_memory" 2 5;
 .timescale -9 -10;
P_0092467C .param/l "ADDR_WIDTH" 2 7, +C4<0100>;
P_00924690 .param/l "WIDTH" 2 8, +C4<01100>;
v00923440_0 .net "ADDR", 3 0, v00924498_0; 1 drivers
v00923498_0 .var "CLK", 0 0;
v0092B290_0 .net "DATA", 11 0, v00923880_0; 1 drivers
v0092B2E8_0 .var "RST", 0 0;
S_00929920 .scope module, "p1" "program_counter" 2 15, 3 4, S_00928F90;
 .timescale -9 -10;
P_0092A044 .param/l "WIDTH" 3 10, +C4<0100>;
v00924440_0 .net "CLK", 0 0, v00923498_0; 1 drivers
v00924498_0 .var "DATA", 3 0;
v009233E8_0 .net "RST", 0 0, v0092B2E8_0; 1 drivers
E_0092A020 .event posedge, v00924440_0;
S_009291B0 .scope module, "mem" "memory" 2 21, 4 1, S_00928F90;
 .timescale -9 -10;
P_0092A45C .param/l "ADDR_WIDTH" 4 6, +C4<0100>;
P_0092A470 .param/l "WIDTH" 4 7, +C4<01100>;
v00923828_0 .alias "ADDR", 3 0, v00923440_0;
v00923880_0 .var "DATA", 11 0;
v009243E8 .array "MEM", 0 15, 11 0;
E_0092A1E0 .event edge, v00923828_0;
    .scope S_00929920;
T_0 ;
    %wait E_0092A020;
    %load/v 8, v009233E8_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v00924498_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v00924498_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v00924498_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_009291B0;
T_1 ;
    %wait E_0092A1E0;
    %ix/getv 3, v00923828_0;
    %load/av 8, v009243E8, 12;
    %ix/load 0, 12, 0;
    %assign/v0 v00923880_0, 0, 8;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_009291B0;
T_2 ;
    %vpi_call 4 15 "$readmemb", "rom_test.mem", v009243E8;
    %end;
    .thread T_2;
    .scope S_00928F90;
T_3 ;
    %ix/load 0, 1, 0;
    %assign/v0 v00923498_0, 0, 0;
T_3.0 ;
    %delay 50, 0;
    %load/v 8, v00923498_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v00923498_0, 0, 8;
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_00928F90;
T_4 ;
    %ix/load 0, 1, 0;
    %assign/v0 v0092B2E8_0, 0, 1;
    %delay 200, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v0092B2E8_0, 0, 0;
    %end;
    .thread T_4;
    .scope S_00928F90;
T_5 ;
    %vpi_call 2 37 "$dumpfile", "tb_memory.vcd";
    %vpi_call 2 38 "$dumpvars", 1'sb0, S_00928F90;
    %vpi_call 2 39 "$dumpon";
    %end;
    .thread T_5;
    .scope S_00928F90;
T_6 ;
    %delay 2000, 0;
    %vpi_call 2 43 "$finish";
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "tb_memory.v";
    "./program_counter.v";
