#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_008140E8 .scope module, "tb_decoder2" "tb_decoder2" 2 3;
 .timescale 0 0;
v00FF3410_0 .var "EN", 0 0;
v00FF3468_0 .net "OUT", 1 0, v008134C8_0; 1 drivers
v00FF34C0_0 .var "SEL", 0 0;
S_008133E8 .scope module, "dec2" "decoder2" 2 9, 3 1, S_008140E8;
 .timescale 0 0;
v00813470_0 .net "EN", 0 0, v00FF3410_0; 1 drivers
v008134C8_0 .var "OUT", 1 0;
v00FF33B8_0 .net "SEL", 0 0, v00FF34C0_0; 1 drivers
E_00FF1A88 .event edge, v00813470_0, v00FF33B8_0;
    .scope S_008133E8;
T_0 ;
    %wait E_00FF1A88;
    %load/v 8, v00813470_0, 1;
    %jmp/0xz  T_0.0, 8;
    %load/v 8, v00FF33B8_0, 1;
    %cmpi/u 8, 0, 1;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 1, 1;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.2 ;
    %movi 8, 1, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v008134C8_0, 0, 8;
    %jmp T_0.4;
T_0.3 ;
    %movi 8, 2, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v008134C8_0, 0, 8;
    %jmp T_0.4;
T_0.4 ;
    %jmp T_0.1;
T_0.0 ;
    %ix/load 0, 2, 0;
    %assign/v0 v008134C8_0, 0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_008140E8;
T_1 ;
    %set/v v00FF34C0_0, 0, 1;
    %set/v v00FF3410_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_008140E8;
T_2 ;
    %delay 10, 0;
    %set/v v00FF34C0_0, 1, 1;
    %delay 10, 0;
    %set/v v00FF3410_0, 1, 1;
    %delay 10, 0;
    %set/v v00FF34C0_0, 0, 1;
    %delay 10, 0;
    %set/v v00FF3410_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 25 "$finish";
    %end;
    .thread T_2;
    .scope S_008140E8;
T_3 ;
    %vpi_call 2 29 "$dumpfile", "tb_decoder2.vcp";
    %vpi_call 2 30 "$dumpvars", 1'sb0, S_008140E8;
    %vpi_call 2 31 "$dumpon";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_decoder2.v";
    "./decoder2.v";
