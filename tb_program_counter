#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00811730 .scope module, "tb_program_counter" "tb_program_counter" 2 3;
 .timescale -9 -12;
v00819790_0 .var "CLK", 0 0;
v008197E8_0 .net "DATA", 3 0, v00814588_0; 1 drivers
v00819840_0 .var "RST", 0 0;
S_00814500 .scope module, "p1" "program_counter" 2 9, 3 4, S_00811730;
 .timescale -9 -12;
P_008180DC .param/l "WIDTH" 3 10, +C4<0100>;
v008192D0_0 .net "CLK", 0 0, v00819790_0; 1 drivers
v00814588_0 .var "DATA", 3 0;
v008145E0_0 .net "RST", 0 0, v00819840_0; 1 drivers
E_008180F8 .event posedge, v008192D0_0;
    .scope S_00814500;
T_0 ;
    %wait E_008180F8;
    %load/v 8, v008145E0_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_0.0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v00814588_0, 0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v00814588_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v00814588_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00811730;
T_1 ;
    %set/v v00819790_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/v 8, v00819790_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v00819790_0, 0, 8;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_00811730;
T_2 ;
    %ix/load 0, 1, 0;
    %assign/v0 v00819840_0, 0, 1;
    %delay 20000, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v00819840_0, 0, 0;
    %end;
    .thread T_2;
    .scope S_00811730;
T_3 ;
    %delay 200000, 0;
    %vpi_call 2 25 "$finish";
    %end;
    .thread T_3;
    .scope S_00811730;
T_4 ;
    %vpi_call 2 28 "$dumpfile", "tb_program_counter.vcd";
    %vpi_call 2 29 "$dumpvars", 1'sb0, S_00811730;
    %vpi_call 2 30 "$dumpon";
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    ".\tb_program_counter.v";
    ".\program_counter.v";
