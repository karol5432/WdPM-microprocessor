P_0111803C .param/l "WIDTH" 3 12, +C4<0100>;
v01114B38_0 .net "CE", 0 0, v00F83C30_0; 1 drivers
v01114B90_0 .net "CLK", 0 0, v00F83C88_0; 1 drivers
v01114BE8_0 .net "IN", 3 0, v00F83CE0_0; 1 drivers
v00F83B80_0 .var "OUT", 3 0;
v00F83BD8_0 .net "RST", 0 0, C4<z>; 0 drivers
E_01117E98 .event posedge, v01114B90_0;
    .scope S_01113278;
T_0 ;
    %wait E_01117E98;
    %load/v 8, v01114B38_0, 1;
    %cmpi/u 8, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %load/v 8, v00F83B80_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v00F83B80_0, 0, 8;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v01114BE8_0, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v00F83B80_0, 0, 8;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_011131F0;
T_1 ;
    %set/v v00F83CE0_0, 0, 4;
    %set/v v00F83C30_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v00F83C88_0, 0, 0;
T_1.0 ;
    %delay 5000, 0;
    %load/v 8, v00F83C88_0, 1;
    %inv 8, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v00F83C88_0, 0, 8;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_011131F0;
T_2 ;
    %delay 20000, 0;
    %movi 8, 5, 4;
    %set/v v00F83CE0_0, 8, 4;
    %delay 20000, 0;
    %set/v v00F83C30_0, 1, 1;
    %delay 20000, 0;
    %set/v v00F83C30_0, 0, 1;
    %movi 8, 9, 4;
    %set/v v00F83CE0_0, 8, 4;
    %delay 20000, 0;
    %set/v v00F83C30_0, 1, 1;
    %delay 20000, 0;
    %vpi_call 2 36 "$finish";
    %end;
    .thread T_2;
    .scope S_011131F0;
T_3 ;
    %vpi_call 2 40 "$dumpfile", "tb_register.vcd";
    %vpi_call 2 41 "$dumpvars", 1'sb0, S_011131F0;
